{
    "manifest_version": 3,
    "name": "Gradescope Wrapped",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
          "js": ["content.js"],
          "matches": [
            "https://www.gradescope.com/courses/*"
          ]
        }
    ],
    "background": {
      "service_worker": "service_worker.js"
    },
    "permissions": [
      "activeTab"
    ]
}
